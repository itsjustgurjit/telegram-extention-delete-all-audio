{
    "manifest_version": 2,
    "name": "Telegram Audio Deleter",
    "version": "1.1.13",
    "description": "Delete all audio messages in Telegram Web",
    "permissions": ["https://web.telegram.org/*"],
    "content_scripts": [
      {
        "matches": ["https://web.telegram.org/*"],
        "js": ["content.js"]
      }
    ]
  }
